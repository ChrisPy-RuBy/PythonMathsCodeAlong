   "source": [
    "from sympy import init_printing, symbols, Matrix, sin, cos, pi\n",
    "init_printing()\n",
    "theta = symbols('theta')\n",
    "vect = Matrix([[cos(theta), -sin(theta)], [sin(theta), cos(theta)]])\n",
    "vect"
   ]
